<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="497.0" prefWidth="431.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.javafxphonecontacts.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="titleText" text="Friends Manager" textFill="BLUE">
        <font>
            <Font size="18.0" />
        </font></Label>
    <TextField fx:id="nameTextField" text="Enter a name" />
    <Button id="showFriendsButton" fx:id="showFriendsButton" onAction="#onShowFriendsButtonClick" text="Show Friends" />
    <VBox prefHeight="200.0" prefWidth="100.0">
        <children>
            <ListView id="listView" fx:id="listView" minHeight="-Infinity" prefHeight="200.0" prefWidth="200.0">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </ListView>
        </children>
    </VBox>
    <Button fx:id="clearAllButton" mnemonicParsing="false" text="Clear All" />
    <Label prefHeight="18.0" prefWidth="364.0" text="Messages" />
</VBox>
